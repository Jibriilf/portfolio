!function(){function e({svgElement:e,size:t,color:n,backgroundColor:o,circle:i}){return new Promise((r=>{const c=document.createElement("canvas"),a=c.getContext("2d"),l=t/8,d=6*l;e.setAttribute("width",d),e.setAttribute("height",d),e.querySelectorAll("path").forEach((e=>{e.setAttribute("fill",o)})),c.width=t,c.height=t,i||(a.fillStyle=o,a.fillRect(0,0,c.width,c.height)),a.fillStyle=n,a.beginPath(),a.arc(t/2,t/2,d/2-1,0,2*Math.PI),a.closePath(),a.fill();const s=new window.Blob([e.outerHTML],{type:"image/svg+xml"}),m=URL.createObjectURL(s),u=document.createElement("img");u.src=m,u.width=d,u.height=d,u.onload=()=>{a.drawImage(u,l,l),c.toBlob((e=>{URL.revokeObjectURL(m);const t=new window.FileReader;t.readAsDataURL(e),t.onloadend=()=>{r(t.result)}}))}}))}window.addEventListener("load",(()=>{if(!("serviceWorker"in window.navigator))return;const{logo:t,siteTitle:n,adminUrl:o}=window.wpAdminManifestL10n,i={name:n,display:"standalone",orientation:"portrait",start_url:o,scope:o,icons:[]},{color:r,backgroundColor:c}=function(){const e=document.createElement("div");e.id="wpadminbar",document.body.appendChild(e);const{color:t,backgroundColor:n}=window.getComputedStyle(e);return document.body.removeChild(e),{color:t||"white",backgroundColor:n||"black"}}(),a=function(e){const t=document.implementation.createHTMLDocument("");t.body.innerHTML=e;const{firstElementChild:n}=t.body;return n.setAttribute("viewBox","0 0 80 80"),n}(t);Promise.all([...[180,192,512].map((t=>e({svgElement:a,size:t,color:r,backgroundColor:c}).then((e=>{i.icons.push({src:e,sizes:t+"x"+t,type:"image/png",purpose:"maskable"}),180===t&&function(e,t){const n=document.createElement("link");n.rel="apple-touch-icon",n.href=t,n.sizes="180x180",document.head.insertBefore(n,document.head.firstElementChild)}(0,e)})))),...[180,192,512].map((t=>e({svgElement:a,size:t,color:r,backgroundColor:c,circle:!0}).then((e=>{i.icons.push({src:e,sizes:t+"x"+t,type:"image/png",purpose:"any"})}))))]).then((()=>{!function(e){const t=docu